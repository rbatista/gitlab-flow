#!/usr/bin/env bash

BASEDIR=$(dirname $0)

source $BASEDIR/gitlab-common.sh

start(){
    echo "Start..."
    BRANCH=${1:-$(get_current_branch)}
    echo $BRANCH
    echo $(get_project_id)
}

help(){
    echo "$0:       Magane a gitlab flow features."
    echo "  help: Show this message."
}

command=${1:help}
shift
case "$command" in
        s | start) start $@ ;;
        h | help) help ;;
        *) help ;;
esac
